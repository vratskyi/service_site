{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve"
  },
  "strictNullChecks": false,
  "plugins": [
    {
      "name": "@astrojs/ts-plugin"
    },
  ],
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"],
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"]
  },
  "allowJs": true,
}

